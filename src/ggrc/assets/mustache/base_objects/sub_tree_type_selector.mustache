<tree-type-selector>
  <li>
    <a href="#" class="dropdown-toggle tview-dropdown-toggle" data-toggle="dropdown">
      <i class="grcicon-child-node"></i>
    </a>
    <div class="dropdown-menu dropdown-menu2 dropdown-menu-form">
      <a class="pull-right close-dropdown"><i class="grcicon-x-grey"></i></a>
      <h5>Select object type for {{model.title_singular}}</h5>

      <ul class="model-list">
        {{#child_tree_model_list}}
        <li class="{{#if mandatory}}disabled{{/if_equals}}">
          <label class="checkbox-inline">
            <input type="checkbox"
              {{#if display_status}}checked="checked"{{/if}}
              value="{{model_name}}"
              class="notify-wrap model-checkbox"
            >
            {{display_name}}
          </label>
        </li>
        {{/child_tree_model_list}}
      </ul>

      <a class="btn btn-small btn-info set-display-object-list" href="javascript://">Show Objects</a>
    </div>
  </li>
</tree-type-selector>
