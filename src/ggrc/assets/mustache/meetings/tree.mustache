
  <li class="tree-item governance" data-object-id="{{instance.id}}" data-object-type="{{instance.class.table_singular}}">
    <div class="item-main" {{#instance}}{{data 'model'}}{{/instance}}>
      <div class="row-fluid">
        <div class="span12">
                      
          <div class="item-data">
            <div class="tree-title-area">
              <i class="grcicon-calendar"></i>
                  <a class="url" data-popover-trigger="sticky-hover" data-sticky-popover="true" data-content="{{render '/static/mustache/pbc/iframe_tooltip.mustache' url=instance.title}}" href="{{schemed_url instance.title}}" target="_blank">
                    {{instance.title}}
                  </a>
              {{{!render_hooks 'Meeting.tree_view_info'}}}
            </div>
          </div>
        </div>
      </div>
      
    </div>
  </li>

