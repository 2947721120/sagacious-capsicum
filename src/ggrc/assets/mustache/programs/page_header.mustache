<h1 class="entities">
  <span class="title-prev">
    <a class="title-trigger" href="/dashboard"><i class="grciconlarge-dashboard-new"></i></a>
    My Dashboard
  </span>
  <span class="title-slide">
    <i class="grciconlarge-{{model.table_singular}}"></i>
    {{instance.title}}
  </span>
  <a class="nav-trigger active" href="javascript://">
    <i class="grciconlarge-nav-trigger" rel="tooltip" data-placement="left" title="" data-original-title="Hide menu"></i>
  </a>
  {{#if instance.private}}
  <span class="private">
    <i class="grcicon-private" rel="tooltip" title="Private"></i>
  </span>
  {{/if}}
</h1>
