{{#each widget_list}}
  {{^if_match count '0'}}
    <li>
      <a href="{{ selector }}" rel="tooltip" data-placement="bottom" title="{{ internav_display }}">
        <div class="oneline">
          <i class="{{ internav_icon }}"></i>
          {{#has_count}}{{count}}{{#spinner}}...{{/spinner}}{{/has_count}}
        </div>
      </a>
      <div class="drag"></div>
    </li>
  {{/if_match}}
{{/each}}

{{{render_hooks 'ObjectNav.Actions'}}}
