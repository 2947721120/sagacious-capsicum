{{#each widget_list}}
  <li>
    <a href="{{ selector }}">
      <div class="oneline">
        <i class="{{ internav_icon }}"></i>
        {{ internav_display }}
        {{#has_count}}({{count}}{{#spinner}}...{{/spinner}}){{/has_count}}
      </div>
    </a>
    <div class="drag"></div>
  </li>
{{/each}}

{{{render_hooks 'ObjectNav.Actions'}}}
