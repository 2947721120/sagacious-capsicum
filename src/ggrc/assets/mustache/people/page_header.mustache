<h1 class="{{model.category}}">

  {{#is_dashboard}}
    <i class="grciconlarge-dashboard-new"></i>
    My Work
    <a class="nav-trigger active" href="javascript://">
      <i class="grciconlarge-nav-trigger" rel="tooltip" data-placement="left"
        title="" data-original-title="Hide menu">
      </i>
    </a>
  {{else}}
    <div class="grcobject {{model.category}}" rel="tooltip" data-placement="bottom"
      title="{{model.title_singular}}">
      <i class="grcicon-{{model.table_singular}}"></i>
    </div>
  	{{firstnonempty instance.name instance.email}}
  {{/is_dashboard}}
</h1>
