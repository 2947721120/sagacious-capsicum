{{#list}}
<li class="{{class.category}}" data-model="true" {{data 'model'}}>
  {{#using program=program}}
  <a href="{{program.viewLink}}#audit_widget" class="show-extended">
    <div>
      <div>
        <span class="lhs-item">
          {{title}}
        </span>
      </div>
      <div class="lhs-private">
      <i class="grcicon-private" rel="tooltip" title="Private"></i>
        {{#is_private}}
          <i class="grcicon-private" rel="tooltip" title="Private"></i>
        {{/is_private}}
      </div>
    </div>
  </a>
  {{/using}}
</li>
{{/list}}
{{^list}}
<li class="spinner">
</li>
{{/list}}
{{#if_helpers '\
   ^is_allowed' 'read' model.shortName _0_context='any' '\
   and ^is_allowed' 'create' model.shortName _1_context='any' '\
   and ^is_allowed' 'read' 'Program' _2_context='any'}}
<li class="no-access">
  You do not have access to read this information.
</li>
{{/if_helpers}}
{{#list.is_loading}}
<li class="spinner">
  <span {{attach_spinner '{ "radius": 4, "length": 4, "width": 2 }' 'display:inline-block; top: -3px; left: 30px;' }}></span>
</li>
{{/list.is_loading}}
