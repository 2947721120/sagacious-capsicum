  <li data-object-id="{{instance.id}}" data-object-type="{{instance.class.table_singular}}">
    <div class="item-main objective-selector field-wrap" data-model="true" {{#instance}}{{data "model"}}{{/instance}}>
      <a href="javascript://">
        <i class="grcicon-{{instance.class.table_singular}}-color"></i>
        {{instance.title}}
      </a>

      {{#if allow_mapping}}
      {{#if mappings}}
      {{#is_allowed_all 'delete' mappings}}
      <a href="javascript://" data-toggle="unmap" data-id="{{instance.id}}" data-type="{{instance.class.shortName}}" class="deleteTrigger">
        <i class="grcicon-deleted"></i>
        {{#result}}<span class="result" {{data 'result'}}></span>{{/result}}
      </a>
      {{/is_allowed_all}}
      {{/if}}
      {{/if}}
    </div>
  </li>
