  <li data-object-id="{{instance.id}}" data-object-type="{{instance.class.table_singular}}">
    <div class="item-main objective-selector field-wrap" data-model="true" {{#instance}}{{data "model"}}{{/instance}}>
      {{#with_page_object_as 'page_instance'}}
      {{#with_mapping "objects" page_instance}}
      <ggrc-quick-update instance="mappings.0" parent_instance="page_instance" source_mapping="objects">
        <input tabindex="3" type="text" name="object" class="editTitle search-icon autocomplete" placeholder="Select Object" value="{{instance.title}}" data-mapping="objects" {{mapping_autocomplete}}>
      </ggrc-quick-update>
      {{/with_mapping}}
      {{/with_page_object_as}}

      {{#if allow_mapping}}
      {{#if mappings}}
      {{#is_allowed_all 'delete' mappings}}
      <a href="javascript://" data-toggle="unmap" data-id="{{instance.id}}" data-type="{{instance.class.shortName}}" class="deleteTrigger"><i class="grcicon-deleted"></i>
        {{#result}}<span class="result" {{data 'result'}}></span>{{/result}}
      </a>
      {{/is_allowed_all}}
      {{/if}}
      {{/if}}
    </div>
  </li>
