<ul class="tree-structure simple single-row-tabs">
{{#list}}
  <li data-model="true" {{data 'person'}} class="{{object_type}}">
    <a>
      <div class="row-fluid">
        <div class="span12">
          <div class="item-identifier">
            <span class="title">
              {{person.email}}
            </span>
            <span class="role">
              {{#roles}}
              {{name}}
              {{/roles}}
            </span>
          </div>
        </div>
      </div>
    </a>
  </li>
{{/list}}
</ul>
